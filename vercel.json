{
    "version": 2,
    "functions": {
        "/": {
          "memory": 1024,
          "includeFiles": "/server/db.json"
        }
    },
    "routes": [
      {
        "src": "/api/progress",
        "dest": "/api/progress"
      },
      {
        "src": "/api/slack/(.*)",
        "dest": "https://slack.com/$1"
      },
      {
        "src": "/callback",
        "dest": "/"
      }
    ]
  }