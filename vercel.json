{
    "version": 2,
    "functions": {
        "/": {
          "memory": 1024
        }
    },
    "routes": [
      {
        "src": "/api/progress",
        "dest": "/api/progress"
      },
      {
        "src": "/api/slack/(.*)",
        "dest": "https://slack.com/$1"
      },
      {
        "src": "/callback",
        "dest": "/"
      }
    ],
    "static": {
      "directory": "server"
    }
}